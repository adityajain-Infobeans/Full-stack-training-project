        <h4 class="mt-3 text-center">Login Successful</h4>
        <h6 class="mt-4 text-center">
          <strong>Welcome ! </strong>
          <script>
            document.write(localStorage.getItem("projectUserEmail"));
          </script>
        </h6>
      </div>
    </div>
